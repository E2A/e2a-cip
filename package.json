{
  "name": "TARP",
  "version": "0.2.0",
  "private": true,
  "description": "A tool to analyze CIP and other AYSHR resources. Developed by Bivee Inc for Pathfinder & E2A.",
  "author": "Bivee Inc <getstarted@bivee.co> (http://bivee.co)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "icons": "svgstore -o './src/assets/images/spritemap.svg' --inline './src/assets/images/icons/**/*.svg'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preicons": "yarn run svgo",
    "svgo": "svgo -f './src/assets/images/icons/' --disable removeViewBox --enable removeDimensions",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "background.js",
  "dependencies": {
    "@vuex-orm/core": "^0.32.0",
    "blob": "^0.0.5",
    "bourbon": "^6.0.0",
    "chartist": "0.10.1",
    "country-list": "^2.1.0",
    "currencies-symbol": "^1.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "file-saver": "^2.0.2",
    "fs": "^0.0.1-security",
    "include-media": "^1.4.9",
    "jquery": "^3.4.1",
    "json-loader": "^0.5.7",
    "locale2": "^2.3.1",
    "marked": "^0.7.0",
    "modularscale-sass": "^3.0.5",
    "normalize.css": "^8.0.0",
    "papaparse": "^5.0.0",
    "register-service-worker": "^1.0.0",
    "vee-validate": "^2.1.0-beta.7",
    "vue": "^2.5.16",
    "vue-analytics": "^5.16.0",
    "vue-i18n": "^8.0.0",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.1.3",
    "vue-select": "^3.1.0",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "yaml-loader": "^0.5.0",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "electron": "^9.4.0",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "svg-inline-loader": "^0.8.0",
    "svgo": "^1.0.5",
    "svgstore-cli": "^1.3.1",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "ie >= 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "SEE LICENSE IN LICENSE and CONTENT_LICENSE.MD",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
